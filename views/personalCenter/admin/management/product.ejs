<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Author</th>
            <th>Type</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        <% for( let i=0; i < product.length; i++ ) { %>
            <tr>
                <td><%= product[i].name %></td>
                <td><%= product[i].author %></td>
                <td><%= product[i].type %></td>
                <td><%= product[i].price %></td>
            </tr>
        <% } %>
    </tbody>
</table>