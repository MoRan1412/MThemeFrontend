<thead>
    <tr>
        <th colspan="3">Search</th>
    </tr>
</thead>
<tbody>
    <% for( let i=0; i < comment.length; i++ ) { %>
        <tr onclick="location.href='/admin/commentManage/<%= comment[i].id %>'">
            <td class="userManageTd1">
                <div class="userManageTd1Avatar" style="background-image: url('<%= comment[i].avatar %>');"></div>
                <%= comment[i].username %>
            </td>
            <td class="manageDataContainer">
                <div class="manageData" style="text-transform: none;"><span style="text-transform: uppercase;">Content</span><%= comment[i].content %></div>
                <div class="manageData"><span >Status</span><%= comment[i].status %></div>
            </td>
            <td><i class="material-icons">arrow_forward_ios</i></td>
        </tr>
    <% } %>
</tbody>